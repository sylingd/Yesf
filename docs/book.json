{
	"title": "Yesf",
	"author": "ShuangYa",
	"plugins": [
		"-lunr",
		"-search",
		"search-pro",
		"-sharing",
		"github-buttons",
		"editlink",
		"-highlight",
		"prism",
		"anchor-navigation-ex",
		"splitter",
		"copy-code-button",
		"expandable-chapters-small",
		"3-ba"
	],
	"pluginsConfig": {
		"search-pro": {
			"cutWordLib": "nodejieba",
			"defineWord": [
				"Gitbook Use"
			]
		},
		"anchor-navigation-ex": {
			"showLevel": false
		},
		"github-buttons": {
			"buttons": [{
				"user": "sylingd",
				"repo": "Yesf",
				"type": "star",
				"size": "small",
				"count": true
			}]
		},
		"editlink": {
			"base": "https://github.com/riskers/gitbook-template/blob/master",
			"label": "Edit",
			"multilingual": false
		},
		"prism": {
			"ignore": [
				"mermaid",
				"eval-js"
			],
			"css": [
				"prismjs/themes/prism-solarizedlight.css"
			]
		},
		"3-ba": {
			"token": "44b41bc63385a6b27e692272bb1fb393"
		}
	}
}